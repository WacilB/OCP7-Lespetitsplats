{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "no-unused-vars": "warn",
        "no-console": "off",
        "func-names": "off",
        "no-process-exit": "off",
        "object-shorthand": "off",
        "class-methods-use-this": "off",
        "typeof": "off",
        "no-tabs": 0,
        "no-undef": "off",
        "no-param-reassign": "off",
        "no-lonely-if": "off",
        "eol-last": "off",
        "no-plusplus": "off",
        "no-shadow": "off",
        "no-case-declarations": "off"
    }
}